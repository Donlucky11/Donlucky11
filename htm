<script>
    function getRandomAmount(currency) {
        const amountRanges = {
            '£': { min: 899, max: 3847665 },
            '$': { min: 1745, max: 3364773 },
            '€': { min: 1600, max: 4637110 },
            '₹': { min: 38738, max: 1748993 },
            'R$': { min: 23833, max: 8456330 },
            'NT$': { min: 70745, max: 46372017 },
            'CAD': { min: 473, max: 9473045 },
            'MEX': { min: 89356, max: 18635449 },
            '₦': { min: 48935, max: 13026647 },
            'RM': { min: 48365, max: 16478306 },
            'A$': { min: 5488, max: 27486556 },
            'ARS': { min: 803647, max: 26757499 },
            'HKD': { min: 45637, max: 2537649 },
            '₱': { min: 122996, max: 22463093 },
            'ZAR': { min: 33627, max: 15039645 }
        };

        const { min, max } = amountRanges[currency] || { min: 0, max: 10000 };
        const randomAmount = Math.floor(Math.random() * (max - min + 1)) + min;
        return numberWithCommas(randomAmount.toFixed(0));
    }

    function numberWithCommas(x) {
        return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    }

    function getRandomCurrency() {
        const currencies = ['£', '$', '€', '﷼', '₹', 'R$', 'NT$', 'CAD', 'MEX', '₦', '₨', 'RM', 'A$', 'ARS', 'HKD', '₱', 'ZAR'];
        const randomIndex = Math.floor(Math.random() * currencies.length);
        return currencies[randomIndex];
    }

    function getRandomWallet() {
        const wallets = ['Binance', 'Coinbase', 'LUNO', 'CashApp', 'OKX', 'PayPal', 'Blockchain', 'SuperEx', 'AltCoinTrader', 'VARL'];
        const randomIndex = Math.floor(Math.random() * wallets.length);
        return wallets[randomIndex];
    }

    function generateRandomTransaction() {
        const container = document.getElementById('transaction-container');

        const newTransaction = document.createElement('div');
        newTransaction.classList.add('transaction');

        const wallet = getRandomWallet();
        const currency = wallet === 'CashApp' ? '$' : getRandomCurrency();
        const amount = getRandomAmount(currency);
        const date = new Date().toLocaleDateString();
        const time = new Date().toLocaleTimeString();

        newTransaction.innerHTML = `
            <div style="color: yellow;">${wallet}</div>
            <div><span style="color: yellow;">${currency}</span><span style="color: #ffffff">${amount}</span></div>
            <div style="color: #ffffff;">${date}</div>
            <div style="color: #ffffff;">${time}</div>
        `;

        container.insertBefore(newTransaction, container.firstChild);

        // Keep only the first 5 transactions, remove the rest
        if (container.children.length > 3) {
            container.removeChild(container.lastChild);
        }
    }

    setInterval(generateRandomTransaction, 2000);
</script><br><br>


